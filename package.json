{
  "name": "cheat-sheet-6502-generator",
  "version": "1.0.0",
  "description": "6502/65C02 CPU instruction cheat sheet generator",
  "main": "scripts/generate-direct.js",
  "scripts": {
    "generate": "node scripts/generate-direct.js",
    "generate-pdf": "node scripts/generate-direct.js --format=pdf",
    "generate-png": "node scripts/generate-direct.js --format=png",
    "generate-both": "node scripts/generate-direct.js --format=both",
    "generate-html": "node scripts/generate-html.js",
    "generate-all": "npm run generate-both && npm run generate-html"
  },
  "keywords": [
    "6502",
    "65C02",
    "cpu",
    "assembly",
    "cheat-sheet",
    "reference"
  ],
